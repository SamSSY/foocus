{
  "name": "寶寶不浪費時間Lite",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "blocking time-wasting websites to make you focus on working",
  "icons": { "1": "icon.jpg" },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "寶寶不浪費時間Lite",
    "default_icon": "icon.jpg"//,
    //"default_popup": "popup.html"
  }
}